{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/TcpMux/TcpMux.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/TcpMux/TcpMux.csproj",
                "-f",
                "netcoreapp2.1",
                "-o",
                "${workspaceFolder}/bin/publish"
            ]
        }
    ]
}